<?xml version="1.0" encoding="ISO-8859-1"?>
<project>
  <extend>${basedir}/../project.xml</extend>
  <pomVersion>3</pomVersion>
  <artifactId>acegi-security-sandbox</artifactId>
  <name>Acegi Security System for Spring - Sandbox</name>
  <siteDirectory>/home/groups/a/ac/acegisecurity/htdocs/multiproject/acegi-security-sandbox</siteDirectory>
  <repository>
    <connection>scm:svn:https://svn.sourceforge.net/svnroot/acegisecurity/trunk/acegisecurity</connection>
    <developerConnection>scm:svn:https://svn.sourceforge.net/svnroot/acegisecurity/trunk/acegisecurity</developerConnection>
    <url>http://svn.sourceforge.net/viewcvs.cgi/acegisecurity/trunk/acegisecurity/sandbox/</url>
  </repository>
  <dependencies>
    <dependency>
      <groupId>org.acegisecurity</groupId>
      <artifactId>acegi-security</artifactId>
      <version>1.0.0</version>
      <type>jar</type>
      <url>http://acegisecurity.org</url>
    </dependency>
    <dependency>
      <groupId>ldapsdk</groupId>
      <artifactId>ldapsdk</artifactId>
      <version>4.1</version>
      <type>jar</type>
    </dependency>    
  </dependencies>
  <build>
    <resources>
      <resource>
        <directory>${basedir}/../</directory>
        <targetPath>META-INF</targetPath>
        <includes>
          <include>notice.txt</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
</project>

